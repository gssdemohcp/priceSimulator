<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form">
	<Panel expandable="true" expanded="true" headerText="Material Costs" width="auto" backgroundDesign="Solid" class="customPanel">
		<f:Form id="FormToolbar" editable="false" class="formBotton">
			<f:layout class="formBotton">
				<f:ResponsiveGridLayout labelSpanS="6" emptySpanL="0" emptySpanM="0" labelSpanM="7" labelSpanL="7" columnsL="3" columnsM="3"
					class="formBotton"/>
			</f:layout>
			<f:formContainers >
				<f:FormContainer>
					<f:formElements>
						<f:FormElement label="Standard Cost">
							<f:fields>
								<Text text="{simulationModel>/StandardCost}"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Cost Unit">
							<f:fields>
								<Text text="{simulationModel>/Losgr} {simulationModel>/Meins}"/>
							</f:fields>
						</f:FormElement>
					</f:formElements>
				</f:FormContainer>
				<f:FormContainer class="formBotton">
					<f:formElements>
						<f:FormElement label="FOB Cost">
							<f:fields>
								<Text text="{simulationModel>/Fob}"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Freight">
							<f:fields>
								<Text text="{simulationModel>/Freight}"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Overhead">
							<f:fields>
								<Text text="{simulationModel>/Overhead}"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Others">
							<f:fields>
								<Text text="{simulationModel>/Others}"/>
							</f:fields>
						</f:FormElement>
					</f:formElements>
				</f:FormContainer>
				<f:FormContainer>
					<f:formElements>
						<f:FormElement label="Custom Fees">
							<f:fields>
								<Text text="{simulationModel>/CustomFees}"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Custom Duty">
							<f:fields>
								<Text text="{simulationModel>/CustomDuty}"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Currency">
							<f:fields>
								<Text text="{simulationModel>/FwaerKpf}"/>
							</f:fields>
						</f:FormElement>
					</f:formElements>
				</f:FormContainer>
			</f:formContainers>
		</f:Form>
	</Panel>
</core:FragmentDefinition>